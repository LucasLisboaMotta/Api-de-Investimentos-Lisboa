{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
    "plugin:@typescript-eslint/recommended",
    "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "import/no-unresolved": "off",
      "import/extensions": "off",
      "lines-between-class-members": "off"
    }
}
